{
  "name": "Label Studio | AI4ME Annotation Tool",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "label-studio",
  "workspaceFolder": "/app",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "Label Studio",
      "onAutoForward": "openBrowser"
    }
  },
  "postStartCommand": "chmod +x /app/post_start.sh && /app/post_start.sh",
  "mounts": [
    {
      "source": "${localWorkspaceFolder}/data",
      "target": "/app/data",
      "type": "bind"
    },
    {
      "source": "${localWorkspaceFolder}/config",
      "target": "/app/config",
      "type": "bind"
    },
    {
      "source": "${localWorkspaceFolder}/storage",
      "target": "/app/storage",
      "type": "bind"
    }
  ]